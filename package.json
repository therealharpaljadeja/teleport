{
  "name": "monad-teleport",
  "version": "0.1.0",
  "description": "React component for bridging USDC to Monad mainnet via Li.fi",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsup && npm run build:css",
    "build:css": "tailwindcss -i ./src/styles/teleport.css -o ./dist/styles.css --minify",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "monad",
    "bridge",
    "usdc",
    "lifi",
    "cross-chain",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@lifi/sdk": "^3.4.1",
    "clsx": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
